    "@babel/preset-typescript": "^7.13.0",
    "@hotwired/stimulus": "^3.0.1",
    "@types/lodash.throttle": "^4.1.6",
    "np": "^7.5.0",
    "prettier-standard": "16.4.1",
    "typescript": "^4.2.4",
    "vite": "^2.3.4"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.1"
  },
  "dependencies": {
    "lodash.throttle": "^4.1.1"
  }
